{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/playback/**",
        "function": "playbackContainer"
      },
      {
        "source": "/dialogue/**",
        "function": "host"
      }
    ]
  }
}
